    <!-- Definition of output files
   output_freq: ts is physics time steps, d is day
   enabled=".true." to output a file
      -->

    <file_definition>
    
        <!-- Instantaneous outputs; 10/Td (if day_step=24000 and ts=80) -->
        <file id="ins-10perTd"
              name="Xins-10perTd"
              output_freq="30ts" 
              type="one_file"
              enabled=".false.">
                    
            <!-- VARS 2D
            <field_group operation="instant"
                         freq_op="1ts">
                <field field_ref="ISR_TOA" />
                <field field_ref="OLR_TOA" />
            </field_group> -->

            <!-- VARS 3D -->
            <field_group operation="instant"
                         freq_op="1ts">
                <field field_ref="temp" />
                <field field_ref="p" />
                <field field_ref="u" />
                <field field_ref="v" />
                <field field_ref="w" />
            </field_group>
        </file>

        <!-- averages outputs; every Tday -->
        <file id="diurnalave"
              name="Xdiurnalave"
              output_freq="1d" 
              type="one_file"
              output_level="3"
              enabled=".false.">
                    
            <!-- VARS 0D -->
            <field_group operation="average"
                         freq_op="1ts">
                <field field_ref="ls" level="2" />
                <field field_ref="Declin" level="3" />
            </field_group>

            <!-- VARS 2D -->
            <field_group operation="average"
                         freq_op="1ts">
                <field field_ref="area" level="3" operation="once" />
                <field field_ref="tsurf" level="1" />
                <field field_ref="ps" level="1" />
            </field_group>

            <!-- VARS 3D -->
            <field_group operation="average"
                         freq_op="1ts">
                <field field_ref="temp" level="1" />
                <field field_ref="teta" level="2" />
                <field field_ref="p" level="1" />
                <field field_ref="pphi" level="1" />
                <field field_ref="u" level="1" />
                <field field_ref="v" level="1" />
                <field field_ref="w" level="2" />
                <field field_ref="HCN_tot" level="3" />
                <field field_ref="C2H2_tot" level="3" />
                <field field_ref="C2H6_tot" level="3" />
                <field field_ref="AC6H6_tot" level="3" />
            </field_group>
        </file>

        <!-- Output file for DragonFly model intercomp - every Tday -->
        <file id="intercomp.diurn"
              name="Xintercomp.diurn"
              output_freq="1d" 
              type="one_file"
              output_level="1"
              enabled=".true.">
                    
            <!-- VARS 0D -->
            <field_group operation="average"
                         freq_op="1ts">
                <field field_ref="ls" level="1" />
            </field_group>

            <!-- VARS 2D Time-Indep-->
            <field_group operation="average"
                         freq_op="1ts">
                <field field_ref="area" level="1" operation="once" />
                <field field_ref="pphis" level="1" operation="once" />
            </field_group>

            <!-- ~~~~~~~ VARS 2D Time-Dep ~~~~~~ -->
            <!-- Average -->
            <field_group operation="average"
                         freq_op="1ts">
