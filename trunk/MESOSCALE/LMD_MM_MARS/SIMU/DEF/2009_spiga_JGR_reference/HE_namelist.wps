 j_parent_start    =   1, 40
 e_we              =  150, 181
 e_sn              =  150, 181
 geog_data_res     =  '32ppd','32ppd'
 dx = 50000,
 dy = 50000,
 map_proj = 'mercator',
 ref_lat   =  -10.,
 ref_lon   =  100.,
 truelat1  =  0.0,
 truelat2  =  0.0,
 stand_lon =  00.00,
 geog_data_path = './WPS_GEOG',
/

&metgrid
 fg_name = './WPSFEED/OXF'
 io_form_metgrid = 2,
 opt_output_from_metgrid_path='./WRFFEED'
/
