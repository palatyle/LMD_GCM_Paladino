&share
 wrf_core = 'ARW',
 max_dom = 2,
 start_date = '2025-07-02_00:00:00'  '2026-01-47_00:00:00'
! end_date   = '2025-07-15_00:00:00', '2026-01-47_00:00:00'
 end_date   = '2025-07-03_00:00:00'
 interval_seconds = 7400
 io_form_geogrid = 2,
 debug_level = 0,
 opt_output_from_geogrid_path='./'
/


&geogrid
