&share
 wrf_core = 'ARW',
 max_dom = 1,
 start_date = '2024-06-02_00:00:00', '2026-01-47_00:00:00'
 end_date   = '2024-06-07_00:00:00', '2026-01-47_00:00:00'
 interval_seconds = 3700
 io_form_geogrid = 2,
 debug_level = 0,
 opt_output_from_geogrid_path='./'
/


&geogrid
