 dy = 20000, 
 map_proj = 'lambert' !!'mercator',
 ref_lat   =  32. !!19.0996,
 ref_lon   =  -40. !!-33.2526,
 truelat1  =  30.,
 truelat2  =  0.0,
 stand_lon =  -33.,
 geog_data_path = './WPS_GEOG',
/

&metgrid
 fg_name = './WPSFEED/LMD'
 io_form_metgrid = 2,
 opt_output_from_metgrid_path='./WRFFEED'
/
