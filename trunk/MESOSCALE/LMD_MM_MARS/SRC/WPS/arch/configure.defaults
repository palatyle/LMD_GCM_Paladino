F77FLAGS	=	-qfixed
FNGFLAGS	=	$(FFLAGS) -qextname
LDFLAGS		=	-Wl,-stack_size,10000000,-stack_addr,0xc0000000
CC		=	gcc-3.3 # NCAR silly thing
CC		=	cc
SCC		=	$(CC)
CFLAGS		=	-O
CPP		=	/usr/bin/cpp -C -P
CPPFLAGS	=	-DAIX -DMAC -DIO_NETCDF -DIO_BINARY -DIO_GRIB1 -DBIT32
RANLIB		=	ranlib

